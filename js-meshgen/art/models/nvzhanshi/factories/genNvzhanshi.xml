			<options rotate_x_axis="yes" flip_textures="no" />
			<animation file="/art/models/nvzhanshi/textures/attack.caf" name="attack" type="travel"
				base_vel="0" min_vel="1" max_vel="0" min_random="10" max_random="30" />
			<animation file="/art/models/nvzhanshi/textures/back.caf" name="back" type="travel"
				base_vel="0" min_vel="1" max_vel="0" min_random="10" max_random="30" />
